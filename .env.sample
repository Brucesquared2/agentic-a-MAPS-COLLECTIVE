# Copy to .env and fill in secrets or environment-specific values
# Example values (do NOT commit real secrets)
CLAUDE_API_KEY=
LEDGER_PATH=logs/key_log.yml
# any other envs: OLlama_PORT, QWEN_PORT, etc.

# Set PUBLISH_AUTH_TOKEN to secure the cockpit publish endpoint
# Example: PUBLISH_AUTH_TOKEN=supersecret123
PUBLISH_AUTH_TOKEN=

# Optional override for the publish endpoint URL
# COCKPIT_PUBLISH_URL=http://localhost:3000/api/cockpit/publish

# Optional token used by orchestrator when forwarding
COCKPIT_PUBLISH_TOKEN=

# For compatibility: set both if you want the same secret name in different contexts
# PUBLISH_AUTH_TOKEN=supersecret123
# COCKPIT_PUBLISH_TOKEN=supersecret123
